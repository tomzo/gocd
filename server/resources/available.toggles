{
   "version": "1",
   "toggles": [
      {
         "key": "pipeline_comment_feature_toggle_key",
         "description": "Enables commenting on a pipeline run, in the pipeline history view. See more at: http://www.go.cd/documentation/user/current/beta/comment_on_pipeline_run.html. Default: off.",
         "value": false
      },
      {
         "key": "plugin_upload_feature_toggle_key",
         "description": "Enables uploading a plugin, directly from Go's plugins tab. Default: off.",
         "value": false
      },
      {
         "key": "color_logs_feature_toggle_key",
         "description": "Enables colored logs in job detail. Default: on.",
         "value": true
      },
      {
         "key": "new_cctray_feature_toggle_key",
         "description": "Enables new cctray implementation. See more at: https://github.com/gocd/gocd/pull/877. Default: on.",
         "value": true
      },
      {
         "key": "api_requests_short_session_feature_toggle_key",
         "description": "Enables short-lived sessions for API requests. See more at: https://github.com/gocd/gocd/issues/496. Default: on.",
         "value": true
      }
   ]
}
